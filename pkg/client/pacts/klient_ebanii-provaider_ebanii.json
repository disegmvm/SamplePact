{
  "consumer": {
    "name": "klient_ebanii"
  },
  "provider": {
    "name": "provaider_ebanii"
  },
  "interactions": [
    {
      "description": "User 'Alice' is requested",
      "providerState": "User Alice exists",
      "request": {
        "method": "GET",
        "path": "/users/1",
        "matchingRules": {
          "$.path": {
            "match": "regex",
            "regex": "\\/users\\/[0-9]+"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
        },
        "body": {
          "id": "1",
          "firstName": "Alice",
          "lastName": "Doe"
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}